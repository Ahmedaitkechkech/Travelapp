                                        <!-- Title -->
                                        <h4><%= ticket.heure_depart %></h4>
                                        <h6 class="mb-0"><%= ticket.Date_depart %></h6>
                                        <p class="mb-0"><%= ticket.lieu_depart %></p>
                                    </div>
    
                                    <!-- Airport detail -->
                                    <div class="col-sm-4 col-md-3">
                                        <!-- Title -->
                                        <h4><%= ticket.heure_arrivee %></h4>
                                        <h6 class="mb-0"><%= ticket.Date_arrivee %></h6>
                                        <p class="mb-0"><%= ticket.lieu_arrivee %></p>
                                    </div>
    
                                    <!-- Price -->
                                    <div class="col-sm-4 col-md-3 text-center">
                                        <!-- Price -->
                                        <h4 value="<%= ticket.prix %>">$<%= ticket.prix %></h4>
                                        <a href="/flight-detail" class="btn btn-dark">Book Now</a>
                                        <button class="btn btn-link text-decoration-underline p-0 mb-0" data-bs-toggle="modal" data-bs-target="#flightdetail">
                                            <i class="bi bi-eye-fill me-1"></i>Flight Details
                                        </button>
                                    </div>
    
                                </div>
                            </div>
                            <!-- card-body END -->
    
                            <!-- card footer -->
                            <div class="card-footer custom-card-footer p-0 mx-3 my-3">
                                <div class="bg-light p-2 rounded-2">
                                    <ul class="list-inline d-sm-flex justify-content-sm-between mb-0 mx-4">
                                        <li class="list-inline-item text-danger">Only 10 Seat Left</li>
                                        <li class="list-inline-item">Total time: 10h 35m</li>
                                        <li class="list-inline-item text-success">Refundable</li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <!-- Ticket item END -->
                    <% }); %>
                </div>
            </div>
            <!-- Main content END -->
        </div> <!-- Row END -->
    </div>
</section>

    <!-- =======================
    Flight list END -->
    
    </main>
